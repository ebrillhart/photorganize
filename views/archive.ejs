<main>
	<!-- *********************** -->
    <!-- photo display container -->
    <!-- *********************** -->
    <div class="container" id="archiveCon">
        <div class ="row">
            <% photos.forEach(function(photo) { %>
            <div class="col m3">
                <div class="card z-depth-3">
                    <div class="card-image waves-effect waves-block waves-light">
                        <img class="activator" src="<%= photo.thumbnail %>" id="<%= photo.id %>">
                    </div>
                    <div class="card-reveal valign-wrapper">
                        <div class="row">
                            <span class="card-title grey-text text-darken-4"><i class="material-icons right">close</i></span>
                        </div>
                        <div class="row valign-wrapper center-align">
                            <div class="col s6 m6 l6 offset-s3 offset-m3 offset-l3">
                                <p class="center-align vertical-align"><a href="<%= '/user/' + photo.userId + '/archive/' + photo.id %>">Details</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
</main>
