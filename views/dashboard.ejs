<div class="container">
	<div class="col m12">
		<!-- search container -->
		<div class="row valign-wrapper">
			<div class="col m1 offset-m2">
				<i class="material-icons vertical-align right">search</i>
			</div>
			<div class ="col m6">
				<form>
	        		<div class="input-field">
	         			<input id="search" type="search" placeholder="Search by Tag" required>
	        		</div>
	      		</form>
      		</div>
		</div>
		<!-- photo display container -->
		<div class ="row">
			<% photos.forEach(function(photo) { %>
				<div class="col m3">
					<div class="card z-depth-3">
    					<div class="card-image waves-effect waves-block waves-light">
      						<img class="activator" src="<%= photo %>">
    					</div>
    					<div class="card-reveal valign-wrapper">
    						<div class="row">
      							<span class="card-title grey-text text-darken-4"><i class="material-icons right">close</i></span>
      						</div>
      						<div class="row">
      							<p class="center-align vertical-align"><a href="/user/:id/photo/:idx">Details</a></p>
      							<p class="center-align"><a href="#">Archive</a></p>
      						</div>
    					</div>
  					</div>
				</div>
			<% }); %>
		</div>	
	</div>
	<div class ="col m3">
	</div>
</div>
